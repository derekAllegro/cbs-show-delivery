{
  "name": "@cbs-app/cbs-show-delivery",
  "version": "1.0.0",
  "private": true,
  "description": "cbs-show-delivery page",
  "author": "TEAM",
  "homepage": "",
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc",
    "_dev": "cross-env NODE_ENV=development yarn typecheck && webpack serve",
    "dev": "cross-env APP_ENV=dev PORT_PROXYEGRESS=6002 PROXY_HOST=local-development-proxy-dev.allegrogroup.com yarn _dev",
    "dev:vte": "cross-env APP_ENV=phoenix PORT_PROXYEGRESS=6002 PROXY_HOST=local-development-proxy-dev.allegrogroup.com yarn _dev",
    "dev:local": "cross-env LOCAL_DEVELOPMENT=true BASE_BACKEND_URL=http://localhost:8081 NODE_ENV=development yarn _dev",
    "lint": "eslint ./src --ext=js,ts,tsx",
    "lint:fix": "yarn lint --fix",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --coverage",
    "test:watch": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest --watch",
    "test:update-snapshots": "cross-env DO_SNAPSHOTS=1 NODE_ICU_DATA=./node_modules/full-icu jest -u",
    "build": "yarn typecheck && rimraf dist && cross-env NODE_ENV=production webpack",
    "update-types": "ts-node ./update-types.ts",
    "start": "node dist/server.js",
    "migrate": "run-cbs-migrations",
    "prettier:fix": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
    "prettier:graphql": "prettier --config .prettierrc.js 'src/**/*.graphql.ts' --write",
    "postinstall": "cbs postinstall",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "publishConfig": {
    "registry": "https://artifactory.allegrogroup.com/artifactory/api/npm/group-npm/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.allegrogroup.com/pl.allegro.logistics.delivery/cbs-show-delivery.git"
  },
